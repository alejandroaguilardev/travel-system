{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "outDir": "dist",
  "compilerOptions": {
    "plugins": [
      "@nestjs/swagger"
    ],
    "deleteOutDir": true,
    "assets": [
      {
        "include": "**/*.pdf",
        "outDir": "dist/pdf"
      }
    ],
    "watchAssets": true
  }
}