{
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/pdf/(.*)",
      "dest": "/pdf/$1"
    },
    {
      "src": "/(.*)",
      "dest": "src/main.ts"
    }
  ],
  "functions": {
    "src/main.ts": {
      "includeFiles": "dist/pdf/**"
    }
  }
}